{
  "expo": {
    "name": "SAT Groups",
    "slug": "satGroups",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/satgroups/applogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": {
            "scheme": "https",
            "host": "play.google.com",
            
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/satgroups/applogo.png",
        "backgroundColor": "#ffffff"
      },
      // TODO: change before release
      "package": "com.satgroups.app",
      "versionCode": 7,
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/satgroups/applogo.png"
    },
    "plugins": [
      "expo-router",
      [
        
        "expo-splash-screen",
        {
          "image": "./assets/images/satgroups/Splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2cd3171b-3702-44f1-ba5e-b49eca68d346"
      }
    }
  },
  "assetBundlePatterns": [
  "**/*",
  "locales/**/*"
]
}
